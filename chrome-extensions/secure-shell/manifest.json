{
"update_url": "https://clients2.google.com/service/update2/crx",

  "minimum_chrome_version": "33",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "name": "Secure Shell",
  "version": "0.8.29",
  "default_locale": "en",
  "icons": {
    "128": "images/stable/icon-128.png",
    "32": "images/stable/icon-32.png",
    "16": "images/stable/icon-16.png"
  },
  "description": "Terminal emulator and SSH client.",
  "offline_enabled": true,
  "options_page": "html/nassh_preferences_editor.html",
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "notifications",
    "storage",
    "terminalPrivate",
    "unlimitedStorage"
  ],
  "web_accessible_resources": [
    "html/crosh.html",
    "html/nassh.html",
    "html/nassh_google_relay.html",
    "html/nassh_connect_dialog.html"
  ],
  "background": {
    "scripts": [
      "js/nassh_deps.concat.js",
      "js/nassh_background.js",
      "js/nassh.js",
      "js/nassh_app.js",
      "js/nassh_executables.js",
      "js/nassh_preference_manager.js",
      "js/nassh_nassh.js"
    ]
  },
  "app": {
    "launch": {
      "local_path": "html/nassh.html"
    }
  }
}
